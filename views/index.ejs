<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<title>Avengers Initiative</title>
		<link rel="stylesheet" href="css/styles.css" type="text/css">

	</head>
	<body>
		<h1>Avengers Initiative</h1>
		<div id="audio" class="on"></div>
		<div class="avengers">
			<% avengers.forEach((a) => { %>
			<div id="<%= a.id %>" class="avenger hide">
				<p class="name"> <%= a.givenName %> <%= a.familyName %> </p>
				<p class="email"> <%= a.email %> </p>
				<p class="created"> <%= a.created.toDate() %> </p>
			</div>
			<% }) %>
		</div>

		<button class="add hide"><span>Add Avenger</span></button>
		<% include bg %>
		<div id="dim"></div>

		<script src="events.js"></script>
	</body>
</html>